        <listener class-name="supports.ReportConfigurations"/>
    </listeners>
    <parameter name="remote" value="true"/>
    <parameter name="port" value="4445"/>
    <test name="Test">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="testcases.login.LoginCases"/>
            <class name="testcases.home.HomeCases"/>
        </classes>
    </test>
</suite>